{
    "name": "ankitjaiswal/laravel-langman",
    "description": "Manage language files with ease.",
    "keywords": ["laravel", "localization", "multilingual"],
    "homepage": "https://github.com/ankitjaiswal/laravel-langman",
    "license": "MIT",
    "authors": [
        {
            "name": "Ankitjaiswal",
            "email": "ankitjaiswal12@gmail.com",
            "homepage": "https://ankitjaiswal.github.io"
        }
    ],
    "require": {
        "php": "^5.5.9 || ^7.0",
        "illuminate/support": "~5.1",
        "illuminate/console": "~5.1",
        "illuminate/filesystem": "~5.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "^4.8 || ^5.0",
        "orchestra/testbench": "~3.0",
        "orchestra/database": "~3.0",
        "mockery/mockery": "~0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Ankitjaiswal\\Langman\\": "src"
        },
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ankitjaiswal\\Langman\\LangmanServiceProvider"
            ]
        }
    }
}
